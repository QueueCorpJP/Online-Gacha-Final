{
  "name": "gacha-api-project",
  "version": "1.0.0",
  "description": "Backend API for Online Trading Card Gacha Website",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build --webpack",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:generate-migration": "npm run typeorm -- migration:generate -d src/config/typeorm.config.ts",
    "typeorm:create-migration": "npm run typeorm -- migration:create",
    "typeorm:run-migrations": "npm run typeorm -- migration:run -d src/config/typeorm.config.ts",
    "typeorm:revert-migration": "npm run typeorm -- migration:revert -d src/config/typeorm.config.ts"
  },
  "dependencies": {
    "@line/bot-sdk": "^9.7.1",
    "@nestjs/common": "^11.0.9",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.9",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^5.0.2",
    "@nestjs/swagger": "^6.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@paypayopa/paypayopa-sdk-node": "^2.1.0",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/pdfkit": "^0.13.9",
    "@types/stripe": "^8.0.417",
    "D": "^1.0.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fast-csv": "^5.0.2",
    "nanoid": "^4.0.2",
    "nodemailer": "^6.10.0",
    "pdfkit": "^0.16.0",
    "pg": "^8.13.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "stripe": "^17.6.0",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@types/bcrypt": "^5.0.2",
    "typescript": "^4.0.0"
  }
}
